#include <iostream>
using namespace std;

int main() {
    int n = 4;  // Number of rows
    int num = 1;

    for (int i = 1; i <= n; i++) {      // Loop for each row
        for (int j = 1; j <= i; j++) {  // Print i numbers in row i
            cout << num++;
        }
        cout << endl;
    }

    return 0;
}
1
23
456
789
