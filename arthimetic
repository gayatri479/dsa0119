#include <iostream>
using namespace std;
int main() {
    int a, b;
    cout << "Enter two numbers: ";
    cin >> a >> b;
    cout << "add: " << a + b << endl;
    cout << "subtract: " << a - b << endl;
    cout << "multiply: " << a * b << endl;
    if (b != 0) {
        cout << "Divide: " << a / b << endl;
        cout << "Modulo: " << a % b << endl;
    } else {
        cout << "Cannot divide or modulo by zero." << endl;
    }
    return 0;
}
