#include <iostream>
using namespace std;
int main() {
    char ch;
    cout << "Enter an alphabet: ";
    cin >> ch;
    char c = tolower(ch);
    if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u') {
        cout << ch << " is a vowel.\n";
    } else if ((c >= 'a' && c <= 'z')) {
        cout << ch << " is a consonant.\n";
    } else {
        cout << "Invalid input.\n";
    }
    return 0;
}
